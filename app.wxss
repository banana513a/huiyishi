/**app.wxss**/
page{
  background-color: #eee;
  font-size: 12px;
  font-family:"Helvetica Neue",Helvetica,Arial,sans-serif,"微软雅黑","宋体";
}
view,navigator{
    box-sizing: border-box;
}
@font-face {font-family: "iconfont"; /* IE9*/
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-less:before { content: "\e6a5"; }

.icon-moreunfold:before { content: "\e6a6"; }

.icon-account:before { content: "\e6b8"; }

.icon-clock:before { content: "\e6bb"; }

.icon-zuowei:before { content: "\e604"; }

.icon-electronics:before { content: "\e6da"; }

.icon-qrcode:before { content: "\e631"; }

.icon-map:before { content: "\e715"; }

.icon-mobilephone:before { content: "\e72a"; }

.icon-huiyishi:before { content: "\e603"; }

/* 共用的小样式 */
.bg-white{
  background-color: #fff;
}
.clearfix:before, .clearfix:after {
    content: "";
    display: table;
}
.clearfix:after { 
    clear: both; 
}
.pull-left{
    float: left;
}
.pull-right{
    float: right;
}

/* 公告列表样式 */
.noticeList{
  margin-top: 20rpx;
  padding: 0 16rpx;
}
.noticeList-item{
  padding: 24rpx 6rpx 16rpx;
  border-bottom: 1px solid #f3f3f3;
  display: -webkit-box;
	display: -webkit-flex;
  display: flex;
}
.noticeList-item-r{
  padding-left: 8px;
  -webkit-box-flex: 1;
	-webkit-flex: 1;
	flex:1;
  overflow: hidden;
}
.noticeList-item-r text{
  display: block;
}
.noticeList-item-title{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: bold;
  color: #393939;
}
.noticeList-item-desc{
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  text-overflow: -o-ellipsis-lastline;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  color: #999;
  line-height: 1.3;
  margin-top: 5px;
  height: 43px;
}
/* 二维码浮标 */
.qrcode{
  position: fixed;
  right: 10px;
  bottom: 50px;
  color: #b02923;
  font-size: 30px;
  width: 40px;
  height: 40px;
  background-color: #fff;
  text-align: center;
  line-height: 40px;
}
.qrcode span{
  font-size: 30px;
  font-weight: bold;
}